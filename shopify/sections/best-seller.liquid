    <style>
        /* Custom scrollbar styles for desktop carousel */
        .scrollbar-custom::-webkit-scrollbar {
            height: 4px;
        }
        
        .scrollbar-custom::-webkit-scrollbar-track {
            background: #e5e7eb;
            border-radius: 10px;
        }
        
        .scrollbar-custom::-webkit-scrollbar-thumb {
            background: #1f2937;
            border-radius: 10px;
            transition: height 0.3s ease;
        }
        
        .scrollbar-custom:hover::-webkit-scrollbar {
            height: 6px;
        }
        
        .scrollbar-custom::-webkit-scrollbar-thumb:hover {
            background: #111827;
        }

        /* Smooth dropdown animation for mobile */
        .product-grid {
            transition: max-height 0.5s ease-in-out;
        }

        /* Image aspect ratio container */
        .product-image-container {
            position: relative;
            width: 100%;
            padding-bottom: 100%; /* 1:1 Aspect Ratio */
            overflow: hidden;
            background: #f3f4f6;
            border-radius: 10px;
        }

        /* Product card border radius */
        .product-card {
            border-radius: 10px;
        }

        /* Product info padding */
        .product-info {
            padding: 12px;
        }

        /* Container with overflow on right side only (desktop) */
        .container-custom {
            max-width: 1280px; /* Same as max-w-7xl */
            margin-left: auto;
            margin-right: auto;
        }

        @media (min-width: 768px) {
            .container-custom {
                padding-left: 2rem; /* Same as px-8 */
                padding-right: 0; /* No padding on right to allow overflow */
                margin-right: 0; /* Remove right margin */
                max-width: none; /* Remove max width on desktop */
            }
        }

        .product-image-container img {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            object-fit: cover;
            object-position: center;
            transition: opacity 0.3s ease;
        }

        .product-image-container .hover-image {
            opacity: 0;
        }

        .product-card:hover .product-image-container .hover-image {
            opacity: 1;
        }

        .product-card:hover .product-image-container .default-image {
            opacity: 0;
        }

        /* Badge styles */
        .badge {
            font-size: 10px;
            font-weight: 700;
            letter-spacing: 0.5px;
            text-transform: uppercase;
            padding: 4px 12px;
            border-radius: 20px;
            border: 1.5px solid;
        }

        /* Star rating */
        .star-rating {
            color: #000;
        }

        /* Show More button animation */
        .rotate-180 {
            transform: rotate(180deg);
        }

        .transition-transform {
            transition: transform 0.3s ease;
        }

        /* Desktop: Peek effect for 5th card */
        @media (min-width: 768px) {
            .carousel-container {
                display: flex;
                overflow-x: auto;
                scroll-behavior: smooth;
                gap: 1rem;
                padding-bottom: 1rem;
            }

            .product-card {
                flex: 0 0 calc(25% - 0.75rem);
                min-width: 250px;
            }

            /* Show peek of 5th card */
            .carousel-container::after {
                content: '';
                flex: 0 0 calc(12.5% - 0.5rem);
                min-width: 125px;
            }
        }

        /* Mobile: Grid layout */
        @media (max-width: 767px) {
            .mobile-grid {
                display: grid;
                grid-template-columns: repeat(2, 1fr);
                gap: 1rem;
            }
        }
    </style>
</head>
